//the key point is to count the number of 5s
//it is aparent that every appearance of 5 will lead to a 0.
//10=5*2,25=5*5...
//we need to know how many 5s,how many 25s...

public class Solution {
    public int trailingZeroes(int n) {
       int count = 0;
        while (n > 0) {
            n /= 5;
            count += n;
        }
        return count;
    }
}
