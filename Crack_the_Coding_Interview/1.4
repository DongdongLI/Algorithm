/*
Calculate the number of space in the first traverse.

int SpaceUnChanged;

in he second traverse, shift the non-space element; change the space to %20; the index should be i+SpaceUnChanged*2-2

*/

static char[] change(char[] a,int len)
	{
		int numOfSpace=0;
		for(int i=0;i<len;i++)
		{
			if(a[i]==' ')
			{
				numOfSpace++;
			}
		}
		int SpaceUnChanged=numOfSpace;
		for(int i=len-1;i>=0;i--)
		{
			if(a[i]!=' ')
			{
				a[i+SpaceUnChanged*2]=a[i];
			}
			else
			{
				a[i+2*SpaceUnChanged-2]='%';
				a[i+2*SpaceUnChanged-1]='2';
				a[i+2*SpaceUnChanged]='0';
				SpaceUnChanged--;
			}
		}
		System.out.println(a);
		return a;
	}
