/*
  Implement an algorithm to find the kth to last element of a single linked list
*/

/*
  fast and slow pointer
*/

static Node kthTolast(Node head,int k)
	{
		Node fast=head;
		Node slow=head;
		for(int i=0;i<k;i++)
		{
			fast=fast.next;
		}
		while(fast.next!=null)
		{
			slow=slow.next;
			fast=fast.next;
		}
		return slow;
	}
